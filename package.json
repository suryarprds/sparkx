{
  "name": "sparkx",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "vite",
    "server": "tsx server/index.ts",
    "dev:server": "tsx watch server/index.ts",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\" --names \"server,client\" --prefix-colors \"blue,green\"",
    "start": "node dist-server/index.js",
    "start:dev": "npm run server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "db:switch-local": "node scripts/setup-env.js local",
    "db:switch-remote": "node scripts/setup-env.js remote",
    "db:switch": "node scripts/setup-env.js",
    "db:sync": "node scripts/setup-env.js && npm run db:generate && npm run db:push && npm run db:seed"
  },
